class Solution:
    dic = {} 
    def tribonacci(self, n: int) -> int:
        if n == 0:
            return 0
        elif n == 1 or n == 2:
            return 1
        if n in Solution.dic:
            return Solution.dic[n]
        x = self.tribonacci(n-1) + self.tribonacci(n-2) + self.tribonacci(n-3)
        Solution.dic[n] = x
        
        return x

        
